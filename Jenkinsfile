pipeline {
	agent any 
	stages {
		stage("welcome to jenkins") {
			steps {
				script {
					//reading lines from file
					File myfile = new File ("/tmp/test.txt")
					lines = myfile.readLines()
					println lines
					#printing all lines 
					println lines
					#printing each line 
					for(line in lines) {
					println line
					}
					}
					}
				}
			}
		}
		
